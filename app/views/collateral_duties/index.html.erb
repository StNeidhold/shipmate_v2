<div class="page-header">
  <h1>
    Collateral duties
    <a href="/collateral_duties/new" class="btn btn-lg btn-success">New Collateral duty</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Collateral duty name</th>
        <th>Actions</th>
      </tr>

      <% @collateral_duties.each do |collateral_duty| %>
      <tr>
        <td><%= collateral_duty.collateral_duty_name %></td>
        <td>
          <a href="/collateral_duties/<%= collateral_duty.id %>" class="btn btn-primary">Show</a>
          <a href="/collateral_duties/<%= collateral_duty.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_collateral_duty/<%= collateral_duty.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
