<div class="page-header">
  <h1>
    Breakout statement formats
    <a href="/breakout_statement_formats/new" class="btn btn-lg btn-success">New Breakout statement format</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @breakout_statement_formats.each do |breakout_statement_format| %>
      <tr>
        <td><%= breakout_statement_format.description %></td>
        <td>
          <a href="/breakout_statement_formats/<%= breakout_statement_format.id %>" class="btn btn-primary">Show</a>
          <a href="/breakout_statement_formats/<%= breakout_statement_format.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_breakout_statement_format/<%= breakout_statement_format.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @breakout_statement_formats, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
