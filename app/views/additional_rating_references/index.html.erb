<div class="page-header">
  <h1>
    Additional rating references
    <a href="/additional_rating_references/new" class="btn btn-lg btn-success">New Additional rating reference</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Rating</th>
        <th>Rating reference text</th>
        <th>Actions</th>
      </tr>

      <% @additional_rating_references.each do |additional_rating_reference| %>
      <tr>
        <td><%= additional_rating_reference.rating_id %></td>
        <td><%= additional_rating_reference.rating_reference_text %></td>
        <td>
          <a href="/additional_rating_references/<%= additional_rating_reference.id %>" class="btn btn-primary">Show</a>
          <a href="/additional_rating_references/<%= additional_rating_reference.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_additional_rating_reference/<%= additional_rating_reference.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
