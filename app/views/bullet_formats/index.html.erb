<div class="page-header">
  <h1>
    Bullet formats
    <a href="/bullet_formats/new" class="btn btn-lg btn-success">New Bullet format</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @bullet_formats.each do |bullet_format| %>
      <tr>
        <td><%= bullet_format.description %></td>
        <td>
          <a href="/bullet_formats/<%= bullet_format.id %>" class="btn btn-primary">Show</a>
          <a href="/bullet_formats/<%= bullet_format.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_bullet_format/<%= bullet_format.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @bullet_formats, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
